<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxDialog" name="LyricUserDictDialog">
		<id_arg>0</id_arg>
		<style>wxDEFAULT_DIALOG_STYLE|wxRESIZE_BORDER|wxMAXIMIZE_BOX</style>
		<object class="wxGridSizer" variable="GridSizer1" member="no">
			<cols>1</cols>
			<rows>3</rows>
			<object class="sizeritem">
				<object class="wxStaticBoxSizer" variable="StaticBoxSizer1" member="no">
					<label>Label</label>
					<object class="sizeritem">
						<object class="wxTextCtrl" name="ID_TEXTCTRL_NEW_LYRIC" variable="TextCtrlNewLyric" member="yes">
							<value>Text</value>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
							<label>Copy From</label>
						</object>
						<flag>wxALL|wxFIXED_MINSIZE</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxComboBox" name="ID_COMBOBOX_OLD_LYRIC" variable="ComboBoxOldLyric" member="yes" />
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON_ADD_LYRIC" variable="ButtonAddLyric" member="yes">
							<label>Add</label>
							<handler function="OnButtonAddLyricClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxSHAPED|wxFIXED_MINSIZE</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxStaticBoxSizer" variable="StaticBoxSizer2" member="no">
					<label>Label</label>
					<object class="sizeritem">
						<object class="wxGrid" name="ID_GRID1" variable="Grid1" member="yes">
							<cols>2</cols>
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON_DELETE_ROW" variable="ButtonDeleteRow" member="yes">
							<label>Delete Row</label>
							<handler function="OnButtonDeleteRowClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxSHAPED|wxFIXED_MINSIZE</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BoxSizer1" member="no">
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON_LYRIC_OK" variable="ButtonLyricOK" member="yes">
							<label>OK</label>
							<handler function="OnButtonLyricOKClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON_LYRIC_CANCEL" variable="ButtonLyricCancel" member="yes">
							<label>Cancel</label>
							<handler function="OnButtonLyricCancelClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
